{
  "name": "multilingo-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@firebase/firestore": "^1.4.2",
    "@types/cheerio": "^0.22.12",
    "@types/jest": "24.0.13",
    "@types/node": "12.0.2",
    "@types/re-base": "^3.2.0",
    "@types/react": "16.8.18",
    "@types/react-dom": "16.8.4",
    "@types/request": "^2.48.2",
    "@types/request-promise-native": "^1.0.16",
    "cheerio": "^1.0.0-rc.3",
    "connected-react-router": "^6.4.0",
    "firebase": "^6.2.2",
    "lodash": "^4.17.11",
    "node-sass": "^4.12.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "re-base": "^4.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "redux-saga-firebase": "^0.15.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "semantic-ui-react": "^0.87.1",
    "typescript": "3.4.5"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "rm -r coverage; BROWSER=none PORT=80 react-scripts start",
    "build": "node-sass-chokidar src/ -o src/ && react-scripts build",
    "build-css": "node-sass-chokidar src/styles/index.scss -o src/",
    "watch-css": "node-sass-chokidar src/styles/index.scss -o src/ --watch --recursive",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.132",
    "@types/react-redux": "^7.0.9",
    "@types/react-router": "^5.0.1"
  }
}
